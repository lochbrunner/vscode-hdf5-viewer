{
    "name": "vscode-hdf5-viewer",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.51.0"
    },
    "description": "Displays HDF5 files in VS code",
    "scripts": {
        "build-client": "rm -f dist/client/* && webpack --config ./webpack.config.client.js --mode=development",
        "start": "webpack-dev-server --config ./webpack.config.client.js --mode=development --hot --progress --color --port 3003",
        "build-extension": "rm -f dist/extension/* && webpack --devtool nosources-source-map --config ./node-extension.webpack.config.js"
    },
    "keywords": [
        "hdf5",
        "vscode"
    ],
    "author": "Matthias Lochbrunner",
    "publisher": "lochbrunner",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/lochbrunner/react-hdf5-viewer.git"
    },
    "main": "./dist/extension/extension.js",
    "activationEvents": [
        "onCommand:react-hdf5-viewer.openCurrentFile"
    ],
    "contributes": {
        "commands": [
            {
                "command": "react-hdf5-viewer.openCurrentFile",
                "title": "Hdf5 Viewer"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "react-hdf5-viewer.openCurrentFile"
                }
            ]
        },
        "configuration": {
            "title": "Hdf5 Viewer"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
        "@types/classnames": "^2.3.1",
        "@types/node": "^17.0.7",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/vscode": "^1.63.1",
        "@types/webpack": "^5.28.0",
        "@types/webpack-env": "^1.16.3",
        "babel-loader": "^8.2.3",
        "css-loader": "^6.5.1",
        "express": "^4.17.2",
        "html-loader": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.4.5",
        "morgan": "^1.10.0",
        "mustache": "^4.2.0",
        "node-gyp": "^8.4.1",
        "node-loader": "^2.0.0",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.5",
        "postcss-browser-reporter": "^0.6.0",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.2.0",
        "postcss-reporter": "^7.0.5",
        "postcss-url": "^10.1.3",
        "prettier": "^2.5.1",
        "react-refresh": "^0.11.0",
        "sass": "^1.45.2",
        "sass-loader": "^12.4.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.2",
        "yargs": "^17.3.1"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mui/icons-material": "^5.2.5",
        "@mui/material": "^5.2.7",
        "classnames": "^2.3.1",
        "electron-build-env": "^0.2.0",
        "jsfive": "^0.3.5",
        "material-ui-icons": "^1.0.0-beta.36",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
